.partial[data-partial="atoms/deep"] {
  position: relative;
  width: 640px;
  background: red;

  &::before {
    content: "";
    display: block;
    padding-top: calc(9 / 16 * 100%);
  }

  .deep__content {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    padding: 15px;
    box-sizing: border-box;
  }

  &.js--active {
    background: green;
  }
}
